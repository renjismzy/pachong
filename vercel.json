{
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "FEISHU_APP_ID": "@feishu_app_id",
    "FEISHU_APP_SECRET": "@feishu_app_secret",
    "DEEPSEEK_API_KEY": "@deepseek_api_key",
    "SECRET_KEY": "@secret_key"
  }
}